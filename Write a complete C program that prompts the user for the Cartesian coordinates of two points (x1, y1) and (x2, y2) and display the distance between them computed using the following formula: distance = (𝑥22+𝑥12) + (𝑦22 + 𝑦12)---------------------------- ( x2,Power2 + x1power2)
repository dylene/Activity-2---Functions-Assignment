#include <stdio.h>
#include <math.h>
int main()
{
    float x1,x2,y1,y2,distance1,distance2;

    //
    printf("Cartesian Coordinates");
    printf("\nInput x1: ");
    scanf("%f",&x1);
    printf("Input y1: ");
    scanf("%f",&y1);
    printf("Input x2: ");
    scanf("%f",&x2);
    printf("Input y2: ");
    scanf("%f",&y2);

    //
    distance1 = sqrt((pow(x2,2)+pow(x1,2))+(pow(y2,2)+pow(y1,2)));
    printf("The distance between (x1,y1) and (x2,y2) is %.2f",distance1);
    return 0;
}
